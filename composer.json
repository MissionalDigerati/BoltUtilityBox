{
  "name": "missionaldigerati/boltutilitybox",
  "description": "Bolt Utility Box contains various utility methods for Bolt CMS.",
  "type": "bolt-extension",
  "keywords": [
    "twig",
    "utility",
    "methods"
  ],
  "require": {
    "bolt/bolt": "^3.0"
  },
  "scripts": {
    "lint": "phpcs --standard=PSR2 src/",
    "test": "phpunit --colors=always tests/"
  },
  "license": "GPL-3.0+",
  "authors": [
    {
      "name": "Missional Digerati",
      "email": "johnathan@missionaldigerati.org",
      "homepage": "http://missionaldigerati.org"
    }
  ],
  "minimum-stability": "dev",
  "prefer-stable": true,
  "autoload": {
  "psr-4" : {
    "Bolt\\Extension\\MissionalDigerati\\BoltUtilityBox\\" : "src"
  }
},
  "extra": {
      "bolt-assets": "web",
      "bolt-class": "Bolt\\Extension\\MissionalDigerati\\BoltUtilityBox\\BoltUtilityBoxExtension",
      "bolt-icon": "bolt-icon.png",
      "bolt-screenshots": []
  },
  "require-dev": {
    "squizlabs/php_codesniffer": "^3.4",
    "phpunit/phpunit": "^7.5"
  }
}
